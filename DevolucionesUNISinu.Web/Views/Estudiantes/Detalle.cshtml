@model DevolucionesUNISinu.Business.DTOs.Estudiantes.EstudianteDetalleDto

@{
    Layout = null;
}
@section Styles{

}

<form autocomplete="off" id="formulario">
    <div class="form-row">
        <div class="form-group col-md-6">
            <label asp-for="Nombres"></label>
            <input asp-for="Nombres" class="form-control" readonly>
        </div>
        <div class="form-group col-md-6">
            <label asp-for="Apellidos" class="control-label"></label>
            <input asp-for="Apellidos" class="form-control" readonly>
        </div>
    </div>
    <div class="form-row">

        <div class="form-group col-md-6">
            <label asp-for="TipoIdentificacion" class="control-label"></label>
            <input asp-for="TipoIdentificacion" class="form-control" readonly>
        </div>
        <div class="form-group col-md-6">
            <label asp-for="Identificacion" class="control-label"></label>
            <input asp-for="Identificacion" class="form-control" readonly>
        </div>
        <div class="form-group col-md-6">
            <label asp-for="Telefono" class="control-label"></label>
            <input asp-for="Telefono" class="form-control" readonly>

        </div>
    </div>
    <div class="form-row">
        <div class="form-group col-md-12">
            <label asp-for="Facultad" class="control-label"></label>
            <input asp-for="Facultad" class="form-control" readonly>
        </div>

    </div>
    <div class="form-row">
        <div class="form-group col-md-12">
            <label asp-for="Programa" class="control-label"></label>
            <input asp-for="Programa" class="form-control" readonly>
        </div>
    </div>
    <div class="form-row">
        <div class="form-group col-md-12">
            <label asp-for="TipoPrograma" class="control-label"></label>
            <input asp-for="TipoPrograma" class="form-control" readonly>
        </div>
    </div>
    <div class="form-row">
        <div class="form-group col-md-1">
            <label asp-for="Semestre" class="control-label"></label>
            <input asp-for="Semestre" class="form-control" readonly>
        </div>
    </div>

    <div class="form-row">
        <div class="form-group col-md-12">
            <label asp-for="Email" class="control-label"></label>
            <input asp-for="Email" class="form-control" readonly>
        </div>

    </div>
</form>