@model DevolucionesUNISinu.Business.DTOs.Facultades.ProgramaDto

@{
    Layout = null;
}


<form>

    

    <div class="form-group">
        <label asp-for="Nombre" class="control-label"></label>
        <input asp-for="Nombre" class="form-control" disabled /> 
    </div>

    <div class="form-group">
        <label asp-for="NombreFacultad" class="control-label"></label>
        <input asp-for="NombreFacultad" class="form-control" disabled />
    </div>

    <div class="form-group">
        <label asp-for="NombreTipoPrograma" class="control-label"></label>
        <input asp-for="NombreTipoPrograma" class="form-control" disabled />
    </div>


    <div class="form-group">

        <label asp-for="Semestres" class="control-label"></label>
        <input asp-for="Semestres" class="form-control" disabled />
    </div>

    <div class="form-group">
        <div class="row col-md-6 offset-md-3">
            <input type="submit" value="Guardar" class="btn btn-primary btn-block" />
        </div>
    </div>
</form>


<script src="~/js/site.js"></script>
<script src="~/js/select.js"></script>
<script src="~/lib/select2/js/i18n/es.min.js"></script>

@await Html.PartialAsync("_ValidationScriptsPartial")